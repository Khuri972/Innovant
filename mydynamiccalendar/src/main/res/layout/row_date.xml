<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_parrent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/all_cell"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_sub_parrent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_month_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginBottom="15dp"
            android:padding="5dp"
            android:text="01"
            android:textColor="@color/black"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_week_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginBottom="15dp"
            android:padding="5dp"
            android:text="01"
            android:textColor="@color/black"
            android:visibility="gone" />

        <LinearLayout
            android:id="@+id/ll_event_image"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="left|bottom"
            android:layout_marginBottom="1dp"
            android:orientation="vertical">

            <!--<TextView
                android:id="@+id/tv_event_simbol"
                android:layout_width="match_parent"
                android:layout_height="@dimen/event_image_size"
                android:layout_gravity="left|bottom"
                android:layout_marginBottom="1dp"
                android:visibility="visible" />-->

        </LinearLayout>


    </LinearLayout>

</LinearLayout>
